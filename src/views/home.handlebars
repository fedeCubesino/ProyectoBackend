
<h1>Lista de Productos</h1>
<ul>
    {{#each products}}
        <li>
            <h2>{{this.title}}</h2>
            <p>{{this.description}}</p>
            <p>Precio: {{this.price}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Categor√≠a: {{this.category}}</p>
            {{#each this.thumbnails}}
                <img src="{{this}}" alt="{{../title}}" width="100">
            {{/each}}
        </li>
    {{/each}}
</ul>
